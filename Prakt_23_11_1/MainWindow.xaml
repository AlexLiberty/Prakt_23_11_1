<Window x:Class="Prakt_23_11_1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prakt_23_11_1"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
        <TextBox Name="TextBox1" Text="{Binding ElementName=TextBox2, Path=Text}"></TextBox>
        <TextBox Name="TextBox2" Text="{Binding ElementName=TextBox1, Path=Text}"></TextBox>
        </StackPanel>
    </Grid>
</Window>
